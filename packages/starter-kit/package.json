{
  "name": "@pikun/starter-kit",
  "description": "starter kit for tiptap",
  "version": "3.10.1",
  "homepage": "https://tiptap.dev",
  "keywords": [
    "tiptap",
    "tiptap starter kit"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "@pikun/core": "workspace:^",
    "@pikun/extension-blockquote": "workspace:^",
    "@pikun/extension-bold": "workspace:^",
    
    "@pikun/extension-code": "workspace:^",
    "@pikun/extension-code-block": "workspace:^",
    "@pikun/extension-document": "workspace:^",
    
    "@pikun/extension-hard-break": "workspace:^",
    "@pikun/extension-heading": "workspace:^",
    "@pikun/extension-horizontal-rule": "workspace:^",
    "@pikun/extension-italic": "workspace:^",
    "@pikun/extension-link": "workspace:^",
    "@pikun/extension-list": "workspace:^",
    
    "@pikun/extension-ordered-list": "workspace:^",
    "@pikun/extension-paragraph": "workspace:^",
    "@pikun/extension-strike": "workspace:^",
    "@pikun/extension-text": "workspace:^",
    "@pikun/extension-underline": "workspace:^",
    "@pikun/extensions": "workspace:^",
    "@pikun/pm": "workspace:^"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ueberdosis/tiptap",
    "directory": "packages/starter-kit"
  },
  "scripts": {
    "build": "tsup",
    "lint": "prettier ./src/ --check && eslint --cache --quiet --no-error-on-unmatched-pattern ./src/"
  }
}
